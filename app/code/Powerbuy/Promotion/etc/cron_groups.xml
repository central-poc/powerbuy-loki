<?xml version="1.0"?>

<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:DCKAP_Test:etc/cron_groups.xsd">

<group id="mr_cron">

    <schedule_generate_every>1</schedule_generate_every>

    <schedule_ahead_for>4</schedule_ahead_for>

    <schedule_lifetime>30</schedule_lifetime>

    <history_cleanup_every>2880</history_cleanup_every>

    <history_success_lifetime>2880</history_success_lifetime>

    <history_failure_lifetime>2880</history_failure_lifetime>

</group>

</config>